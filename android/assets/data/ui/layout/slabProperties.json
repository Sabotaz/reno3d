{
  "type": "TabWindow",
  "name": "Propriétés de la pièce",
  "width": "22%",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Propriétés",
      "id": "textures",
      "content": [
        {
          "type": "Table",
          "label": "Fonction",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.FONCTION_PIECE",
              "maxLength": 20
            }
          ]
        },
        {
          "type": "TexturePicker",
          "label": "Sol : ",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE1_PICKED",
          "category": "Sol",
          "align": "left",
          "width": "20%",
          "height": "25%"
        },
        {
          "type": "TexturePicker",
          "label": "Murs : ",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE2_PICKED",
          "category": "Mur",
          "align": "left",
          "width": "20%",
          "height": "25%"
        },
        {
          "type": "Table",
          "expand": false,
          "content": [
            {
              "type": "TextButton",
              "text": "Fermer",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.CLOSE",
              "pad": 5
            }
          ]
        }
      ]
    }
  ]
}

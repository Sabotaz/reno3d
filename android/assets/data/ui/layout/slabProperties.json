{
  "type": "TabWindow",
  "name": "Propriétés de la pièce",
  "width": "40%",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Mitoyenneté",
      "id": "mitoyennete",
      "content": [
        {
          "type": "Table",
          "label": "Le plancher bas de la pièce donne sur :",
          "labelPad": 5,
          "labelWidth": 131,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.MITOYENNETE_PLANCHER",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Un vide sanitaire",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlancher.VIDE_SANITAIRE"
                },
                {
                  "type": "CheckBox",
                  "label": "Un terre-plein",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlancher.TERRE_PLEIN"
                },
                {
                  "type": "CheckBox",
                  "label": "Un local non chauffé",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlancher.LOCAL_NON_CHAUFFE"
                },
                {
                  "type": "CheckBox",
                  "label": "Un autre logement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlancher.AUTRE_HABITATION"
                },
                {
                  "type": "CheckBox",
                  "label": "Un autre étage du logement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlancher.AUTRE_ETAGE_DU_LOGEMENT"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Le plafond de la pièce donne sur :",
          "labelPad": 5,
          "labelWidth": 131,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.MITOYENNETE_PLAFOND",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Une toiture",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlafond.TERRASSE"
                },
                {
                  "type": "CheckBox",
                  "label": "Des combles perdues",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlafond.COMBLE_PERDU"
                },
                {
                  "type": "CheckBox",
                  "label": "Des combles amménagées",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlafond.COMBLE_AMMENAGEE"
                },
                {
                  "type": "CheckBox",
                  "label": "Une autre habitation",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlafond.AUTRE_HABITATION"
                },
                {
                  "type": "CheckBox",
                  "label": "Un autre étage du logement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.MitoyennetePlafond.AUTRE_ETAGE_DU_LOGEMENT"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Isolation",
      "id": "isolation",
      "layout": "column",
      "content": [
        {
          "type": "Table",
          "label": "Isolation du plancher :",
          "labelPad": 5,
          //"labelWidth": 138,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.DATE_ISOLATION_PLANCHER",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Inconnue",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.INCONNUE"
                },
                {
                  "type": "CheckBox",
                  "label": "Jamais",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.JAMAIS"
                },
                {
                  "type": "CheckBox",
                  "label": "À la construction",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.A_LA_CONSTRUCTION"
                },
                {
                  "type": "CheckBox",
                  "label": "Avant 1983",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_AVANT_1983"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1983 et 1988",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_1983_ET_1988"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1989 et 2000",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_1989_ET_2000"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 2001 et 2005",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_2001_ET_2005"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 2006 et 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_2006_ET_2012"
                },
                {
                  "type": "CheckBox",
                  "label": "Après 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_APRES_2012"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Isolation du plafond :",
          "labelPad": 5,
          //"labelWidth": 138,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.DATE_ISOLATION_PLAFOND",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Inconnue",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.INCONNUE"
                },
                {
                  "type": "CheckBox",
                  "label": "Jamais",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.JAMAIS"
                },
                {
                  "type": "CheckBox",
                  "label": "À la construction",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.A_LA_CONSTRUCTION"
                },
                {
                  "type": "CheckBox",
                  "label": "Avant 1983",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_AVANT_1983"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1983 et 1988",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_1983_ET_1988"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1989 et 2000",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_1989_ET_2000"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 2001 et 2005",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_2001_ET_2005"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 2006 et 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_ENTRE_2006_ET_2012"
                },
                {
                  "type": "CheckBox",
                  "label": "Après 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.slabproperties.DateIsolationSlab.EN_RENOVATION_APRES_2012"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Textures",
      "id": "textures",
      "content": [
        {
          "type": "TexturePicker",
          "label": "Plancher : ",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE1_PICKED",
          "enum": "fr.limsi.rorqual.core.model.MaterialTypeEnum",
          "align": "left",
          "width": "30%",
          "height": "25%"
        },
        {
          "type": "TexturePicker",
          "label": "Plafond : ",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE2_PICKED",
          "enum": "fr.limsi.rorqual.core.model.MaterialTypeEnum",
          "align": "left",
          "width": "30%",
          "height": "25%"
        }
      ]
    }
  ]
}

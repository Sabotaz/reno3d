{
  "type": "TabWindow",
  "name": "Informations relatives au chauffage",
  "width": "20%",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Équipement",
      "id": "type_chauffage",
      "content": [
        {
          "type": "Table",
          "label": "Source unique de chauffage",
          "labelPad": 5,
          "labelWidth": 219,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.CHAUFFAGE_UNIQUE",
              "content": [
                {
                  "id": "CHAUDIERE_GAZ_STANDARD_APRES_2001",
                  "type": "CheckBox",
                  "label": "Chaudière gaz standard",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.chauffageproperties.Chauffage$Generateur.CHAUDIERE_GAZ_STANDARD_APRES_2001"
                },
                {
                  "id": "CHAUDIERE_GAZ_BASSE_TEMPERATURE_APRES_2001",
                  "type": "CheckBox",
                  "label": "Chaudière gaz basse température",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.chauffageproperties.Chauffage$Generateur.CHAUDIERE_GAZ_BASSE_TEMPERATURE_APRES_2001"
                },
                {
                  "id": "CHAUDIERE_GAZ_CONDENSATION_APRES_2001",
                  "type": "CheckBox",
                  "label": "Chaudière gaz à condensation",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.chauffageproperties.Chauffage$Generateur.CHAUDIERE_GAZ_CONDENSATION_APRES_2001"
                },
                {
                  "id": "CHAUDIERE_ELECTRIQUE",
                  "type": "CheckBox",
                  "label": "Chaudière électrique",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.chauffageproperties.Chauffage$Generateur.CHAUDIERE_ELECTRIQUE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "expand": false,
          "content": [
            {
              "type": "TextButton",
              "text": "Fermer",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.CLOSE",
              "pad": 5
            }
          ]
        }
      ]
    }
  ]
}


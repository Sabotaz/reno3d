{
  "type": "TabWindow",
  "name": "Informations générales du logement",
  "id": "tab_window",
  "width": "80%",
  "content": [
    {
      "type": "Table",
      "name": "Bâtiment",
      "id": "type_batiment",
      "content": [
        {
          "type": "Table",
          "label": "Type de logement : ",
          "labelPad": 5,
          "labelWidth": 180,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_BATIMENT",
              "layout": "column",
              "content": [
                {
                  "type": "ImageButton",
                  "image": "textureTypeBatiment1",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeBatimentEnum.MAISON"
                },
                {
                  "type": "ImageButton",
                  "image": "textureTypeBatiment2",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeBatimentEnum.APPARTEMENT"

        {
          "type": "Table",
          "label": "Nombre de jours/an où\nle logement est inoccupé : ",
          "labelPad": 5,
          "labelWidth": 176,
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.NOMBRE_JOURS_ABSENCE",
              "maxLength": 3

                {
                  "type": "CheckBox",
                  "label": "Ordinateur portable",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.ORDINATEUR_PORTABLE"
                },
                {
                  "type": "CheckBox",
                  "label": "Sèche-linge",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.SECHE_LINGE"

{
  "type": "TabWindow",
  "name": "Informations générales du logement",
  "id": "tab_window",
  "width": "35%",
  "content": [
    {
      "type": "Table",
      "name": "Énergie / Ventilation",
      "id": "energie_logement",
      "content": [
        {
          "type": "Table",
          "label": "Type d'abonnement électrique",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.ABONNEMENT_ELECTRIQUE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Double tarif (heures pleines/heures creuses)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.DOUBLE_TARIF"
                },
                {
                  "type": "CheckBox",
                  "label": "Autre",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.AUTRE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Équipement pour l'éclairage",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.EQUIPEMENT_ECLAIRAGE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Ampoule basse consommation (uniquement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.UNIQUEMENT_BASSE_CONSOMMATION"
                },
                {
                  "type": "CheckBox",
                  "label": "Ampoule basse consommation (majoritairement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.MAJORITAIREMENT_BASSE_CONSOMMATION"
                },
                {
                  "type": "CheckBox",
                  "label": "Ampoule à incandescence (majoritairement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.MAJORITAIREMENT_AMPOULE_A_INCANDESCENCE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Type de ventilation",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_VENTILATION",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Naturelle",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.NATURELLE"
                },
                {
                  "type": "CheckBox",
                  "label": "VMC auto-réglable (installation avant 1982)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_AUTO_REGLABLE_AVANT_1982"
                },
                {
                  "type": "CheckBox",
                  "label": "VMC auto-réglable (installation après 1982)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_AUTO_REGLABLE_APRES_1982"
                },
                {
                  "type": "CheckBox",
                  "label": "VMC Hygro B",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_HYGRO"
                },
                {
                  "type": "CheckBox",
                  "label": "VMC double flux",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_DOUBLE_FLUX"
                },
                {
                  "type": "CheckBox",
                  "label": "Inconnue",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.INCONNUE"
                }
              ]
            }
          ]
        }
      ]
    },
    {

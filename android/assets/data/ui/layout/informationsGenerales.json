          "label": "Catégorie de logement : ",
          "labelPad": 5,
          "labelWidth": 180,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.CATEGORIE_BATIMENT",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "T1 / F1",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.CategorieLogementEnum.T1_F1"

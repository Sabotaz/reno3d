                {
                  "type": "CheckBox",
                  "label": "Entre 1978 et 1982",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.DateConstructionBatimentEnum.ENTRE_1978_ET_1982"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1983 et 1988",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.DateConstructionBatimentEnum.ENTRE_1983_ET_1988"
                },
                {
                  "type": "CheckBox",
                  "label": "Entre 1989 et 2000",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.DateConstructionBatimentEnum.ENTRE_1989_ET_2000"

              "event": "fr.limsi.rorqual.core.event.DpeEvent.ENERGIE_CONSTRUCTION",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Électrique",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEnergieConstructionEnum.ELECTRIQUE"
                },
                {
                  "type": "CheckBox",
                  "label": "Autre",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEnergieConstructionEnum.AUTRE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Type d'abonnement électrique : ",
          "labelPad": 5,
          "labelWidth": 263,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.ABONNEMENT_ELECTRIQUE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Double tarif (heures creuses/heures pleines)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.DOUBLE_TARIF"
                },
                {
                  "type": "CheckBox",
                  "label": "Autre",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.AUTRE"
                }
              ]
            }

        {
          "type": "Table",
          "label": "Présence d'un système\nde climatisation : ",
          "labelPad": 5,
          "labelWidth": 176,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.CLIMATISATION_LOGEMENT",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Oui",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.PresenceClimatisationLogementEnum.OUI"

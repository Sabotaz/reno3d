    {
      "type": "Table",
      "name": "Divers",
      "id": "habitant",
      "content": [
        {
          "type": "Table",
          "label": "Département : ",
          "labelPad": 5,
          "labelWidth": 176,
          "content": [
            {
              "type": "ScrollPaneElement",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.DEPARTEMENT_BATIMENT",
              "enum": "fr.limsi.rorqual.core.dpe.enums.generalproperties.DepartementBatimentEnum",
              "align": "left"
            }
          ]
        },
        {
          "type": "Table",
          "label": "Nombre d'habitants : ",
          "labelPad": 5,
          "labelWidth": 176,
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.NOMBRE_PERSONNES_DANS_LOGEMENT",

                {
                  "type": "CheckBox",
                  "label": "Lave-linge",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.LAVE_LINGE"

                {
                  "type": "CheckBox",
                  "label": "Ordinateur fixe",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.ORDINATEUR_FIXE"

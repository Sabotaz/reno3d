                {
                  "type": "CheckBox",
                  "label": "T3 / F3",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.CategorieLogementEnum.T3_F3"
                },
                {
                  "type": "CheckBox",
                  "label": "T4 / F4",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.CategorieLogementEnum.T4_F4"
                },
                {
                  "type": "CheckBox",
                  "label": "T5 / F5 et +",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.CategorieLogementEnum.T5_F5_ET_PLUS"
                }
              ]
            }
          ]
        }
      ]

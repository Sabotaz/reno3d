{
  "type": "TabWindow",
  "name": "Informations générales du logement",
  "id": "tab_window",
  "width": "23%",
  "content": [
    {
      "type": "Table",
      "name": "Énergie / Ventilation",
      "id": "energie_logement",
      "content": [
        {
          "type": "Table",
          "label": "Type d'abonnement électrique",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.ABONNEMENT_ELECTRIQUE",
              "content": [
                {
                  "id": "DOUBLE_TARIF",
                  "type": "CheckBox",
                  "label": "Double tarif (heures pleines/heures creuses)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.DOUBLE_TARIF"
                },
                {
                  "id": "AUTRE",
                  "type": "CheckBox",
                  "label": "Autre",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeAbonnementElectriqueEnum.AUTRE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Équipement pour l'éclairage",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.EQUIPEMENT_ECLAIRAGE",
              "content": [
                {
                  "id": "UNIQUEMENT_BASSE_CONSOMMATION",
                  "type": "CheckBox",
                  "label": "Ampoule basse consommation (uniquement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.UNIQUEMENT_BASSE_CONSOMMATION"
                },
                {
                  "id": "MAJORITAIREMENT_BASSE_CONSOMMATION",
                  "type": "CheckBox",
                  "label": "Ampoule basse consommation (majoritairement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.MAJORITAIREMENT_BASSE_CONSOMMATION"
                },
                {
                  "id": "MAJORITAIREMENT_AMPOULE_A_INCANDESCENCE",
                  "type": "CheckBox",
                  "label": "Ampoule à incandescence (majoritairement)",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementEclairageEnum.MAJORITAIREMENT_AMPOULE_A_INCANDESCENCE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Type de ventilation",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_VENTILATION",
              "content": [
                {
                  "id": "NATURELLE",
                  "type": "CheckBox",
                  "label": "Naturelle",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.NATURELLE"
                },
                {
                  "id": "VMC_AUTO_REGLABLE_APRES_1982",
                  "type": "CheckBox",
                  "label": "VMC auto-réglable",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_AUTO_REGLABLE_APRES_1982"
                },
                {
                  "id": "VMC_HYGRO",
                  "type": "CheckBox",
                  "label": "VMC Hygro B",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_HYGRO"
                },
                {
                  "id": "VMC_DOUBLE_FLUX",
                  "type": "CheckBox",
                  "label": "VMC double flux",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeVentilationEnum.VMC_DOUBLE_FLUX"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "expand": false,
          "content": [
            {
              "type": "TextButton",
              "text": "Fermer",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.CLOSE",
              "pad": 5
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Électroménager",
      "id": "electromenager",
      "content": [
        {
          "type": "Table",
          "label": "Équipement cuisson",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.EQUIPEMENT_CUISSON",
              "content": [
                {
                  "id": "PLAQUES_MIXTES_ET_FOUR_ELECTRIQUE",
                  "type": "CheckBox",
                  "label": "Plaques mixtes + Four électrique",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementCuissonEnum.PLAQUES_MIXTES_ET_FOUR_ELECTRIQUE"
                },
                {
                  "id": "PLAQUES_MIXTES_ET_FOUR_GAZ",
                  "type": "CheckBox",
                  "label": "Plaques mixtes + Four gaz",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementCuissonEnum.PLAQUES_MIXTES_ET_FOUR_GAZ"
                },
                {
                  "id": "PLAQUES_ELECTRIQUES_ET_FOUR_ELECTRIQUE",
                  "type": "CheckBox",
                  "label": "Plaques électrique + Four électrique",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementCuissonEnum.PLAQUES_ELECTRIQUES_ET_FOUR_ELECTRIQUE"
                },
                {
                  "id": "FEUX_GAZ_ET_FOUR_ELECTRIQUE",
                  "type": "CheckBox",
                  "label": "Feux gaz + Four électrique",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementCuissonEnum.FEUX_GAZ_ET_FOUR_ELECTRIQUE"
                },
                {
                  "id": "FEUX_GAZ_ET_FOUR_GAZ",
                  "type": "CheckBox",
                  "label": "Feux gaz + Four gaz",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementCuissonEnum.FEUX_GAZ_ET_FOUR_GAZ"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "expand": false,
          "content": [
            {
              "type": "TextButton",
              "text": "Fermer",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.CLOSE",
              "pad": 5
            }
          ]
        }
      ]
    }
  ]
}

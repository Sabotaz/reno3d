                {
                  "type": "CheckBox",
                  "label": "Lave-vaisselle",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.LAVE_VAISSELLE"
                },
                {
                  "type": "CheckBox",
                  "label": "Fer à repasser",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.FER_A_REPASSER"
                },
                {
                  "type": "CheckBox",
                  "label": "Réfrigérateur",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.REFRIGERATEUR"
                },
                {
                  "type": "CheckBox",
                  "label": "Congélateur",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.CONGELATEUR"
                },
                {
                  "type": "CheckBox",
                  "label": "Aspirateur",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.generalproperties.TypeEquipementElectromenagerEnum.ASPIRATEUR"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Équipement cuisson",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.EQUIPEMENT_CUISSON",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Plaques mixtes + Four électrique",

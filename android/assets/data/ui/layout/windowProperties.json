{
  "type": "TabWindow",
  "name": "Propriétés de la fenêtre",
  "width": "35%",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Propriétés de la fenêtre",
      "id": "vitrage_et_fermeture",
      "content": [
        {
          "type": "Table",
          "label": "Type de vitre : ",
          "labelPad": 5,
          "labelWidth": 219,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_VITRAGE_MENUISERIE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Simple vitrage",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.SIMPLE_VITRAGE"
                },
                {
                  "type": "CheckBox",
                  "label": "Survitrage",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.SURVITRAGE"
                },
                {
                  "type": "CheckBox",
                  "label": "Double vitrage avant 1990",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.DOUBLE_VITRAGE_INF_1990"
                },
                {
                  "type": "CheckBox",
                  "label": "Double vitrage entre 1990 et 2001",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.DOUBLE_VITRAGE_SUP_1990_INF_2001"
                },
                {
                  "type": "CheckBox",
                  "label": "Double vitrage apres 2001",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.DOUBLE_VITRAGE_SUP_2001"
                },
                {
                  "type": "CheckBox",
                  "label": "Triple vitrage",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeVitrageEnum.TRIPLE_VITRAGE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Type de volets : ",
          "labelPad": 5,
          "labelWidth": 219,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_FERMETURE_MENUISERIE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Aucun",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.SANS_FERMETURE"
                },
                {
                  "type": "CheckBox",
                  "label": "Jalousie accordéon",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.JALOUSIE_ACCORDEON"
                },
                {
                  "type": "CheckBox",
                  "label": "Volet battant avec ajoures fixes",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.VOLET_BATTANT_AVEC_AJOURES_FIXES"
                },
                {
                  "type": "CheckBox",
                  "label": "Volet roulant en métal",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.VOLET_ROULANT_EN_METAL"
                },
                {
                  "type": "CheckBox",
                  "label": "Fermeture sans ajoures",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.FERMETURE_SANS_AJOURES"
                },
                {
                  "type": "CheckBox",
                  "label": "Volet battant bois",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.VOLET_BATTANT_BOIS"
                },
                {
                  "type": "CheckBox",
                  "label": "Volet roulant PVC",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeFermetureEnum.VOLET_ROULANT_PVC"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Masques",
      "id": "vitrage_et_fermeture",
      "content": [
        {
          "type": "Table",
          "label": "Masque proche obstruant le soleil\n(balcon, auvent...)",
          "labelPad": 5,
          "labelWidth": 219,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.MASQUE_PROCHE_MENUISERIE",
              "layout": "column",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Oui",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeMasqueEnum.PRESENCE_MASQUE_PROCHE"
                },
                {
                  "type": "CheckBox",
                  "label": "Non",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeMasqueEnum.ABSENCE_MASQUE_PROCHE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Obstacle masquant le soleil en hiver\n(arbre, autre construction...)",
          "labelPad": 5,
          "labelWidth": 219,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.MASQUE_LOINTAIN_MENUISERIE",
              "layout": "column",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Totalement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeMasqueEnum.PRESENCE_MASQUE_LOINTAIN_TOTAL"
                },
                {
                  "type": "CheckBox",
                  "label": "Partiellement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeMasqueEnum.PRESENCE_MASQUE_LOINTAIN_PARTIEL"
                },
                {
                  "type": "CheckBox",
                  "label": "Pas du tout",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.menuiserieproperties.TypeMasqueEnum.ABSENCE_MASQUE_LOINTAIN"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Modèle",
      "id": "models",
      "content": [
        {
          "type": "ModelPicker",
          "label": "Modèle :",
          "category": "Portes et fenetres",
          "align": "left",
          "width": "30%",
          "height": "20%"
        }
      ]
    },
    {
      "type": "Table",
      "name": "Taille",
      "id": "sizes",
      "content": [
        {
          "type": "Table",
          "label": "Hauteur (en cm)",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.HAUTEUR_MODELE",
              "maxLength": 20
            }
          ]
        },
        {
          "type": "Table",
          "label": "Largeur (en cm)",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.LARGEUR_MODELE",
              "maxLength": 20
            }
          ]
        },
        {
          "type": "CheckBox",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.RATIO_MODELE",
          "label": "Respecter le ratio d'origine",
          "labelFont": "defaultTitle.fnt",
          "labelWidth": "10%"
        },
        {
          "type": "Table",
          "label": "Allège (en cm)",
          "labelPad": 5,
          "labelWidth": "10%",
          "id": "hauteur_au_sol",
          "content": [
            {
              "type": "TextField",
              "channel": "fr.limsi.rorqual.core.event.Channel.UI",
              "event": "fr.limsi.rorqual.core.event.UiEvent.HAUTEUR_SOL_MODELE",
              "maxLength": 20
            }
          ]
        }
      ]
    }
  ]
}

        {
          "type": "TexturePicker",
          "label": "Intérieur 1 :",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE2_PICKED",
          "category": "Mur",
          "align": "left",
          "width": "30%",
          "height": "20%"
        },
        {
          "type": "TexturePicker",
          "label": "Intérieur 2 :",
          "channel": "fr.limsi.rorqual.core.event.Channel.UI",
          "event": "fr.limsi.rorqual.core.event.UiEvent.TEXTURE3_PICKED",
          "category": "Mur",
          "align": "left",
          "width": "30%",
          "height": "20%"

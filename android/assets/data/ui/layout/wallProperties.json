{
  "type": "TabWindow",
  "name": "Propriétés du mur",
  "width": "30%",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Mitoyenneté & orientation",
      "id": "mitoyennete_orientation",
      "content": [
        {
          "type": "Table",
          "label": "Le mur donne sur : ",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_MUR",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Un local non chauffé",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_DONNANT_SUR_UN_LOCAL_NON_CHAUFFE"
                },
                {
                  "type": "CheckBox",
                  "label": "L'extérieur",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_DONNANT_SUR_EXTERIEUR"
                },
                {
                  "type": "CheckBox",
                  "label": "Une autre habitation",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_DONNANT_SUR_UNE_AUTRE_HABITATION"
                },
                {
                  "type": "CheckBox",
                  "label": "Une véranda chauffée",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_DONNANT_SUR_UNE_VERANDA_CHAUFFE"
                },
                {
                  "type": "CheckBox",
                  "label": "Une véranda non chauffée",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_DONNANT_SUR_UNE_VERANDA_NON_CHAUFFE"
                },
                {
                  "type": "CheckBox",
                  "label": "L'intérieur du logement",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.TypeMurEnum.MUR_INTERIEUR"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Orientation : ",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.ORIENTATION_MUR",
              "layout": "column",
              "align": "left",
              "content": [
                {
                  "type": "ImageButton",
                  "image": "textureOrientationNord",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.OrientationEnum.NORD"
                },
                {
                  "type": "ImageButton",
                  "image": "textureOrientationEst",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.OrientationEnum.EST",
                  "retourneLigne": true
                },
                {
                  "type": "ImageButton",
                  "image": "textureOrientationSud",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.OrientationEnum.SUD"
                },
                {
                  "type": "ImageButton",
                  "image": "textureOrientationOuest",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.OrientationEnum.OUEST"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Isolation",
      "id": "isolation_mur",
      "content": [
        {
          "type": "Table",
          "label": "Date de l'isolation : ",
          "labelPad": 5,
          "labelWidth": "10%",
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.DATE_ISOLATION_MUR",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Inconnue",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.INCONNUE"
                },
                {
                  "type": "CheckBox",
                  "label": "Jamais",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.JAMAIS"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée à la construction",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.A_LA_CONSTRUCTION"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée avant 1983",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_AVANT_1983"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée entre 1983 et 1988",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_ENTRE_1983_ET_1988"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée entre 1989 et 2000",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_ENTRE_1989_ET_2000"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée entre 2001 et 2005",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_ENTRE_2001_ET_2005"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée entre 2006 et 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_ENTRE_2006_ET_2012"
                },
                {
                  "type": "CheckBox",
                  "label": "Isolation effectuée après 2012",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.wallproperties.DateIsolationMurEnum.EN_RENOVATION_APRES_2012"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}

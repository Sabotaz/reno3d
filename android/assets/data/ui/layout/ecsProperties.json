{
  "type": "TabWindow",
  "name": "Production d'eau chaude sanitaire du logement",
  "id": "tab_window",
  "content": [
    {
      "type": "Table",
      "name": "Equipement utilisé",
      "id": "equipement_utilise",
      "content": [
        {
          "type": "ButtonGroup",
          "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
          "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_EQUIPEMENT_ECS",
          "content": [
            {
              "type": "TextButton",
              "text": "Source de chauffage primaire",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeEquipementEcsEnum.CHAUFFAGE_PRINCIPAL"
            },
            {
              "type": "TextButton",
              "text": "Source de chauffage secondaire",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeEquipementEcsEnum.CHAUFFAGE_SECONDAIRE"
            },
            {
              "type": "TextButton",
              "text": "Ballon électrique",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeEquipementEcsEnum.BALLON_ELECTRIQUE"
            },
            {
              "type": "TextButton",
              "text": "Chauffe-eau",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeEquipementEcsEnum.CHAUFFE_EAU"
            },
            {
              "type": "TextButton",
              "text": "Accumulateur",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeEquipementEcsEnum.ACCUMULATEUR"
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Type de ballon électrique",
      "id": "type_ballon_electrique",
      "content": [
        {
          "type": "ButtonGroup",
          "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
          "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_BALLON_ELECTRIQUE",
          "content": [
            {
              "type": "TextButton",
              "text": "Ballon électrique horizontal (age < 15ans)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeBallonElectriqueEnum.BALLON_ELECTRIQUE_HORIZONTAL_INF_15ANS"
            },
            {
              "type": "TextButton",
              "text": "Ballon électrique horizontal (age > 15ans)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeBallonElectriqueEnum.BALLON_ELECTRIQUE_HORIZONTAL_SUP_15ANS"
            },
            {
              "type": "TextButton",
              "text": "Ballon électrique vertical (age < 15ans)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeBallonElectriqueEnum.BALLON_ELECTRIQUE_VERTICAL_INF_15ANS"
            },
            {
              "type": "TextButton",
              "text": "Ballon électrique vertical (age > 15ans)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeBallonElectriqueEnum.BALLON_ELECTRIQUE_VERTICAL_SUP_15ANS"
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Type de chauffe-eau",
      "id": "type_chauffe_eau",
      "content": [
        {
          "type": "ButtonGroup",
          "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
          "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_CHAUFFE_EAU",
          "content": [
            {
              "type": "TextButton",
              "text": "Chauffe-eau thermodynamique sur air extrait",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeChauffeEauEnum.CHAUFFE_EAU_THERMODYNAMIQUE_SUR_AIR_EXTRAIT"
            },
            {
              "type": "TextButton",
              "text": "Chauffe-eau thermodynamique sur air extérieur",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeChauffeEauEnum.CHAUFFE_EAU_THERMODYNAMIQUE_SUR_AIR_EXTERIEUR"
            },
            {
              "type": "TextButton",
              "text": "Chauffe-eau gaz (date < 1991)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeChauffeEauEnum.CHAUFFE_EAU_GAZ_INF_1991"
            },
            {
              "type": "TextButton",
              "text": "Chauffe-eau gaz (date entre 1991 et 2002)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeChauffeEauEnum.CHAUFFE_EAU_GAZ_ENTRE_1991_2002"
            },
            {
              "type": "TextButton",
              "text": "Chauffe-eau gaz (date > 2003)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeChauffeEauEnum.CHAUFFE_EAU_GAZ_SUP_2003"
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Type d'accumulateur",
      "id": "type_accumulateur",
      "content": [
        {
          "type": "ButtonGroup",
          "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
          "event": "fr.limsi.rorqual.core.event.DpeEvent.TYPE_ACCUMULATEUR",
          "content": [
            {
              "type": "TextButton",
              "text": "Accumulateur gaz classique (date < 1990)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeAccumulateurEnum.ACCUMULATEUR_GAZ_CLASSIQUE_INF_1990"
            },
            {
              "type": "TextButton",
              "text": "Accumulateur gaz classique (date entre 1990 et 2000)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeAccumulateurEnum.ACCUMULATEUR_GAZ_CLASSIQUE_ENTRE_1990_2000"
            },
            {
              "type": "TextButton",
              "text": "Accumulateur gaz classique (date > 2000)",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeAccumulateurEnum.ACCUMULATEUR_GAZ_CLASSIQUE_SUP_2000"
            },
            {
              "type": "TextButton",
              "text": "Accumulateur gaz condensation",
              "pad" : 2,
              "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.TypeAccumulateurEnum.ACCUMULATEUR_GAZ_CONDENSATION"
            }
          ]
        }
      ]
    },
    {
      "type": "Table",
      "name": "Nombre de personnes vivant dans le logement",
      "id": "nb_personnes",
      "content": [
        {
          "type": "ScrollPaneElement",
          "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
          "event": "fr.limsi.rorqual.core.event.DpeEvent.NOMBRE_PERSONNES_DANS_LOGEMENT",
          "enum": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.NombrePersonnesEnum"
        }
      ]
    },
    {
      "type": "Table",
      "name": "Informations diverses",
      "id": "infos_diverses",
      "content": [
        {
          "type": "Table",
          "label": "Présence d'une installation solaire : ",
          "labelPad" : 5,
          "labelWidth" : 516,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.PRESENCE_INSTALLATION_SOLAIRE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Oui",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.PresenceInstallationSolaireEnum.INSTALLATION_SOLAIRE_PRESENTE"
                },
                {
                  "type": "CheckBox",
                  "label": "Non",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.PresenceInstallationSolaireEnum.INSTALLATION_SOLAIRE_ABSENTE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Le système de production d'eau chaude sanitaire est situé dans un local chauffé : ",
          "labelPad" : 5,
          "labelWidth" : 516,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.LOCAL_EQUIPEMENT_ECS",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Oui",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.LocalEquipementEcsEnum.SITUE_DANS_LOCAL_CHAUFFE"
                },
                {
                  "type": "CheckBox",
                  "label": "Non",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.LocalEquipementEcsEnum.SITUE_DANS_LOCAL_NON_CHAUFFE"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "Usage principal de l'eau chaude : ",
          "labelPad" : 5,
          "labelWidth" : 516,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.USAGE_EAU_CHAUDE",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Douches",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.UsageEauChaudeEnum.DOUCHES"
                },
                {
                  "type": "CheckBox",
                  "label": "Bains",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.UsageEauChaudeEnum.BAINS"
                }
              ]
            }
          ]
        },
        {
          "type": "Table",
          "label": "La chaudière se déclenche à l'ouverture du robinet d'eau chaude : ",
          "labelPad" : 5,
          "labelWidth" : 516,
          "content": [
            {
              "type": "ButtonGroup",
              "channel": "fr.limsi.rorqual.core.event.Channel.DPE",
              "event": "fr.limsi.rorqual.core.event.DpeEvent.DECLENCHEMENT_CHAUDIERE_ROBINET",
              "content": [
                {
                  "type": "CheckBox",
                  "label": "Oui",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.DeclenchementChaudiereEnum.DECLENCHEMENT_OUVERTURE_ROBINET_EAU_CHAUDE"
                },
                {
                  "type": "CheckBox",
                  "label": "Non",
                  "pad": 5,
                  "value": "fr.limsi.rorqual.core.dpe.enums.ecsproperties.DeclenchementChaudiereEnum.NON_DECLENCHEMENT_OUVERTURE_ROBINET_EAU_CHAUDE"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}





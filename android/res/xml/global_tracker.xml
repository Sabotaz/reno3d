<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- How long a session exists before giving up -->
    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">false</bool>

    <!-- Percentage of events to include in reports -->
    <string name="ga_sampleFrequency">100.0</string>

    <!-- catch and report uncaught exceptions from the app -->
    <bool name="ga_reportUncaughtExceptions">true</bool>

    <!-- the Local LogLevel for Analytics -->
    <string name="ga_logLevel">verbose</string>

    <!-- how often the dispatcher should fire -->
    <integer name="ga_dispatchPeriod">30</integer>

    <!-- Treat events as test events and don't send to google -->
    <bool name="ga_dryRun">false</bool>

    <!-- The following value should be replaced with correct property id. -->
    <string name="ga_trackingId">UA-73647708-1</string>

    <screenName name="fr.limsi.rorqual.free.android.AndroidLauncher">Android Launcher Free</screenName>
    <screenName name="fr.limsi.rorqual.android.AndroidLauncher">Android Launcher</screenName>

</resources>
